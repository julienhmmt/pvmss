# Configuration de l'application PVMSS

# Configuration du serveur
PORT=50000
ENV=development

# Configuration de l'authentification
SESSION_SECRET=your_super_secret_key_that_is_at_least_32_bytes_long

# Configuration Proxmox
PROXMOX_URL=https://your-proxmox-ip:8006/api2/json
PROXMOX_API_TOKEN_NAME=your-token-name
PROXMOX_API_TOKEN_VALUE=your-token-value
PROXMOX_VERIFY_SSL=false

# Configuration du cache
CACHE_ENABLED=true
CACHE_TTL=5m
